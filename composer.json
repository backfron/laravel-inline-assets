{
    "name": "backfron/laravel-inline-assets",
    "description": "Render your javascript and css files inline for better performance in production.",
    "license": "MIT",
    "authors": [
        {
            "name": "Jairo Ushi√±a",
            "email": "jago86@gmail.com",
            "homepage": "https://backfron.com"
        }
    ],
    "homepage": "https://github.com/backfron/laravel-inline-assets",
    "keywords": ["Laravel", "LaravelInlineAssets"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Backfron\\LaravelInlineAssets\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backfron\\LaravelInlineAssets\\Tests\\": "tests",
            "App\\": "vendor/orchestra/testbench-core/laravel/app"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Backfron\\LaravelInlineAssets\\LaravelInlineAssetsServiceProvider"
            ],
            "aliases": {
                "LaravelInlineAssets": "Backfron\\LaravelInlineAssets\\Facades\\LaravelInlineAssets"
            }
        }
    }
}
